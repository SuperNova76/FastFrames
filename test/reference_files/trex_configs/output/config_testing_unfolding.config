% ------- %
% - JOB - %
% ------- %

Job: "my_fit"
	HistoPath: .
	Lumi: 1
	ImageFormat: pdf
	ReadFrom: HIST
	POI: mu_signal
	HistoChecks: NOCRASH

% ------- %
% - FIT - %
% ------- %

Fit: "fit"
	FitType: UNFOLDING
	FitRegion: CRSR
	POIAsimov: 1
	FitBlind: True

% ----------- %
% - REGIONS - %
% ----------- %

Region: "Electron_jet_pt"
	Type: SIGNAL
	VariableTitle: jet_pt
	HistoName: NOSYS/jet_pt_Electron
	Label: Electron_jet_pt
	ShortLabel: Electron_jet_pt
	NumberOfRecoBins: 10
	AcceptanceNameSuff: _Electron
	SelectionEffNameSuff: _Electron
	MigrationNameSuff: _Electron

Region: "Muon_jet_pt"
	Type: SIGNAL
	VariableTitle: jet_pt
	HistoName: NOSYS/jet_pt_Muon
	Label: Muon_jet_pt
	ShortLabel: Muon_jet_pt
	NumberOfRecoBins: 10
	AcceptanceNameSuff: _Muon
	SelectionEffNameSuff: _Muon
	MigrationNameSuff: _Muon

% -------------------- %
% - UNFOLDINGSAMPLES - %
% -------------------- %

UnfoldingSample: "ttbar_FS"
	FillColor: 3
	LineColor: 3
	Title: ttbar_FS
	AcceptanceFile: ttbar_FS
	MigrationFile: ttbar_FS
	SelectionEffFile: ttbar_FS
	AcceptanceName: NOSYS/acceptance_eff_particle_jet_pt
	SelectionEffName: NOSYS/selection_eff_particle_truth_jet_pt
	MigrationName: NOSYS/jet_pt_vs_particle_truth_jet_pt

% ----------- %
% - SAMPLES - %
% ----------- %

Sample: "Zjets_light"
	Type: BACKGROUND
	Title: Zjets_light
	HistoFile: Zjets_light
	FillColor: 4
	LineColor: 4
	Exclude: Electron_jet_pt,Muon_jet_pt

Sample: "Zjets_b"
	Type: BACKGROUND
	Title: Zjets_b
	HistoFile: Zjets_b
	FillColor: 5
	LineColor: 5

Sample: "Wjets_FS"
	Type: BACKGROUND
	Title: Wjets_FS
	HistoFile: Wjets_FS
	FillColor: 6
	LineColor: 6
	Exclude: Electron_jet_pt

Sample: "Data"
	Type: DATA
	Title: Data
	HistoFile: Data
	FillColor: 7
	LineColor: 7

% ----------------- %
% - NORM. FACTORS - %
% ----------------- %

NormFactor: "mu_signal"
	Title: ""#mu(signal)""
	Nominal: 1
	Min: -100
	Max: 100
	Samples: ttbar_FS

