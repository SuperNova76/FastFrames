% ------- %
% - JOB - %
% ------- %

Job: "my_fit"
	HistoPath: /home/dubovsky/Analysis/4L/fastframes
	AcceptancePath: /home/dubovsky/Analysis/4L/fastframes
	MigrationPath: /home/dubovsky/Analysis/4L/fastframes
	SelectionEffPath: /home/dubovsky/Analysis/4L/fastframes
	Lumi: 1
	ImageFormat: pdf
	ReadFrom: HIST
	HistoChecks: NOCRASH

% ------- %
% - FIT - %
% ------- %

Fit: "fit"
	FitType: UNFOLDING
	BinnedLikelihoodOptimization: TRUE
	FitRegion: CRSR
	FitBlind: True

% ------------- %
% - UNFOLDING - %
% ------------- %

Unfolding: "particle_truth_jet_pt"
	NumberOfTruthBins: 10
	UnfoldNormXSec: TRUE
	DivideByBinWidth: TRUE
	DivideByLumi: 777777.0
	LogX: FALSE
	LogY: FALSE
	NominalTruthSample: truth_sample_ttbar_FS
	MatrixOrientation: TRUTHONHORIZONTAL

% ----------- %
% - REGIONS - %
% ----------- %

Region: "Electron_jet_pt"
	Type: SIGNAL
	VariableTitle: jet_pt
	HistoName: NOSYS/jet_pt_Electron
	Label: Electron_jet_pt
	ShortLabel: Electron_jet_pt
	NumberOfRecoBins: 10
	AcceptanceNameSuff: _Electron
	SelectionEffNameSuff: _Electron
	MigrationNameSuff: _Electron

Region: "Muon_jet_pt"
	Type: SIGNAL
	VariableTitle: jet_pt
	HistoName: NOSYS/jet_pt_Muon
	Label: Muon_jet_pt
	ShortLabel: Muon_jet_pt
	NumberOfRecoBins: 10
	AcceptanceNameSuff: _Muon
	SelectionEffNameSuff: _Muon
	MigrationNameSuff: _Muon

% -------------------- %
% - UNFOLDINGSAMPLES - %
% -------------------- %

UnfoldingSample: "ttbar_FS"
	FillColor: 3
	LineColor: 3
	Title: ttbar_FS
	AcceptanceFile: ttbar_FS
	MigrationFile: ttbar_FS
	SelectionEffFile: ttbar_FS
	AcceptanceName: NOSYS/acceptance_particle_jet_pt
	SelectionEffName: NOSYS/selection_eff_particle_truth_jet_pt
	MigrationName: NOSYS/jet_pt_vs_particle_truth_jet_pt

% ---------------- %
% - TRUTHSAMPLES - %
% ---------------- %

TruthSample: "truth_sample_ttbar_FS"
	LineColor: 3
	Title: ttbar_FS
	TruthDistributionFile: ttbar_FS
	TruthDistributionName: particle_truth_jet_pt
	TruthDistributionPath: /home/dubovsky/Analysis/4L/fastframes

% ----------- %
% - SAMPLES - %
% ----------- %

Sample: "Zjets_light"
	Type: BACKGROUND
	Title: Zjets_light
	HistoFile: Zjets_light
