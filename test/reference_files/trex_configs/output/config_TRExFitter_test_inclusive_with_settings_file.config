% ------- %
% - JOB - %
% ------- %

Job: "my_fit"
	HistoPath: /home/dubovsky/Analysis/4L/fastframes
	Lumi: 1
	ImageFormat: pdf
	ReadFrom: HIST
	POI: mu_signal
	HistoChecks: NOCRASH

% ------- %
% - FIT - %
% ------- %

Fit: "fit"
	FitType: SPLUSB
	POIAsimov: 1
	FitRegion: CRSR
	FitBlind: True

% ----------- %
% - REGIONS - %
% ----------- %

Region: "Electron_jet_pt"
	Type: SIGNAL
	VariableTitle: jet_pt
	HistoName: NOSYS/jet_pt_Electron
	Label: Electron_jet_pt
	ShortLabel: Electron_jet_pt

Region: "Electron_met_met"
	Type: SIGNAL
	VariableTitle: met_met
	HistoName: NOSYS/met_met_Electron
	Label: Electron_met_met
	ShortLabel: Electron_met_met

Region: "Electron_met_phi"
	Type: SIGNAL
	VariableTitle: met_phi
	HistoName: NOSYS/met_phi_Electron
	Label: Electron_met_phi
	ShortLabel: Electron_met_phi

Region: "Muon_jet_pt"
	Type: SIGNAL
	VariableTitle: jet_pt
	HistoName: NOSYS/jet_pt_Muon
	Label: Muon_jet_pt
	ShortLabel: Muon_jet_pt

Region: "Muon_met_met"
	Type: SIGNAL
	VariableTitle: met_met
	HistoName: NOSYS/met_met_Muon
	Label: Muon_met_met
	ShortLabel: Muon_met_met

Region: "Muon_met_phi"
	Type: SIGNAL
	VariableTitle: met_phi
	HistoName: NOSYS/met_phi_Muon
	Label: Muon_met_phi
	ShortLabel: Muon_met_phi

% ----------- %
% - SAMPLES - %
% ----------- %

Sample: "ttbar_FS"
	Type: Signal
	Title: "t#bar{t}"
	HistoFile: ttbar_FS
	FillColor: 50
	LineColor: 50

Sample: "Wjets"
	Type: BACKGROUND
	Title: Wjets
	HistoFile: Wjets
	FillColor: 4
	LineColor: 4
	Exclude: Electron_met_phi,Muon_met_phi

Sample: "Data"
	Type: DATA
	Title: Data
	HistoFile: Data
	FillColor: 5
	LineColor: 5

% ----------------- %
% - NORM. FACTORS - %
% ----------------- %

NormFactor: "mu_signal"
	Title: "#mu(signal)"
	Nominal: 1
	Min: -100
	Max: 100
	Samples: ttbar_FS

NormFactor: "N_wjets"
	Title: "#mu(W+jets)"
	Nominal: 1
	Min: -100
	Max: 100
	Samples: Wjets

